{
  "name": "subfission/cas",
  "description": "Adds CAS to laravel 5.x",
  "keywords": [
    "CAS",
    "phpCAS",
    "SSO",
    "laravel",
    "laravel 5"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "subfission",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "illuminate/http": "5.x",
    "illuminate/support": "5.x",
    "jasig/phpcas": "~1.3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "Subfission\\Cas\\": "src/Subfission/Cas/"
    },
    "files": [
      "src/Subfission/Cas/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Subfission\\Cas\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Subfission\\Cas\\CasServiceProvider"
      ],
      "aliases": {
        "Cas": "Subfission\\Cas\\Facades\\Cas"
      }
    }
  }
}
